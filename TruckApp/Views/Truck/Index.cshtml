@model List<Truck>

<table class="table">
    <tr>
        <th>Truck Model</th>
        <th></th>
    </tr>

    @foreach(var truck in Model)
    {
        <tr>
            <td>@truck.Model</td>
            <td><a href="@truck.ImageLink"><img src="@truck.ImageLink" style="float:right;width:400px;height:400px;"></a></td>
        </tr>
    }

</table>